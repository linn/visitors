{
    "env": {
        "browser": true,
        "jest": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "jest",
        "react-hooks"
    ],
    "rules": {
        "prettier/prettier": [
        "error"
        ],
        "react/jsx-filename-extension": 0,
        "spaced-comment": "off",
        "indent": ["error", 4],
        "react-hooks/rules-of-hooks": "error",
        "import/no-import-module-exports": "off",
        "react/no-children-prop": "off",
        "react-hooks/exhaustive-deps": "error"
        }
}
